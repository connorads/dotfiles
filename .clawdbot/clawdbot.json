{
  "agents": {
    "defaults": {
      "workspace": "~/clawd",
      "heartbeat": {
        "every": "0m"
      },
      "thinkingDefault": "off",
      "model": {
        "primary": "openai/gpt-5-nano",
        "fallbacks": []
      },
      "models": {
        "openai/gpt-5-nano": {}
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "allowlist",
      "allowFrom": ["${TELEGRAM_ALLOW_FROM}"],
      "tokenFile": "/home/connor/.secrets/telegram-bot-token"
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "type": "token",
      "tokenFile": "/home/connor/.secrets/clawdbot-gateway-token"
    }
  },
  "plugins": {
    "slots": {
      "memory": ""
    }
  },
  "tools": {}
}

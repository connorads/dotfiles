{
  "agents": {
    "defaults": {
      "workspace": "~/clawd",
      "heartbeat": {
        "every": "0m"
      },
      "thinkingDefault": "medium",
      "model": {
        "primary": "openai/gpt-5-nano",
        "fallbacks": ["openai/gpt-4.1-mini", "anthropic/claude-haiku-4.5"]
      },
      "models": {
        "openai/gpt-5-nano": {},
        "openai/gpt-4.1-mini": {},
        "anthropic/claude-haiku-4.5": {}
      },
      "blockStreamingDefault": "on"
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "allowlist",
      "allowFrom": ["${TELEGRAM_ALLOW_FROM}"]
    }
  },
  "gateway": {
    "mode": "local",
    "bind": "loopback",
    "tailscale": {
      "mode": "serve"
    },
    "auth": {
      "allowTailscale": true
    }
  },
  "plugins": {
    "slots": {
      "memory": "memory-core"
    }
  },
  "tools": {}
}

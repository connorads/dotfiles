  "plugin": [
    "opencode-openai-codex-auth@4.2.0",
    "opencode-gemini-auth@1.1.6"
  ],
  "lsp": {
    "pyright": { "disabled": true },
    "ty": {
      "command": ["ty", "server"],
      "extensions": [".py", ".pyi"]
    }
  },
  "tui": {
    "scroll_speed": 10
  },
  "agent": {
    "build": {
      "mode": "primary"
    },
    "plan": {
      "mode": "primary"
    }
  },
